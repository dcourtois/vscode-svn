{
	"name": "vscode-svn",
	"displayName": "SVN",
	"description": "SVN SCM Provider",
	"repository": {
		"type": "git",
		"url": "https://github.com/dcourtois/vscode-svn.git"
	},
	"version": "0.0.1",
	"publisher": "damiencourtois",
	"engines": {
		"vscode": "^1.14.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/src/main",
	"contributes": {
		"commands": [
			{
				"command": "svn.diff",
				"category": "Svn",
				"title": "Diff"
			},
			{
				"command": "svn.stage",
				"category": "Svn",
				"title": "Stage",
				"icon": {
					"light": "resources/icons/light/stage.svg",
					"dark": "resources/icons/dark/stage.svg"
				}
			}
		],
		"menus": {
			"scm/resourceState/context": [
				{
					"command": "svn.stage",
					"group": "inline"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^2.4.2",
		"vscode": "^1.1.4",
		"mocha": "^3.4.2",
		"@types/node": "^8.0.16",
		"@types/mocha": "^2.2.41"
	}
}
